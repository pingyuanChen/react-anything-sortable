{
  "name": "react-anything-sortable",
  "version": "1.3.5",
  "description": "A ReactJS component that can drag and sort any children with IE8 compatibility",
  "main": "lib/index.js",
  "scripts": {
    "watch": "./node_modules/.bin/webpack --config webpack.conf.js",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack --config webpack.conf.js",
    "test": "./node_modules/karma/bin/karma start test/karma.conf.js",
    "demo": "./node_modules/.bin/webpack demo/index.js demo/bundle.js --progress --devtoll source-map --module-bind 'js=babel?stage=0&loose=all' --optimize-minimize --optimize-occurence-order",
    "demo:watch": "./node_modules/.bin/webpack demo/index.js -w demo/bundle.js --progress -d --module-bind 'js=babel?stage=0&loose=all'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jasonslyvia/react-anything-sortable.git"
  },
  "keywords": [
    "react-component",
    "react",
    "sortable"
  ],
  "author": "jasonslyvia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jasonslyvia/react-anything-sortable/issues"
  },
  "homepage": "https://github.com/jasonslyvia/react-anything-sortable",
  "devDependencies": {
    "babel-loader": "~5.3.2",
    "chai": "^2.2.0",
    "chai-spies": "^0.5.1",
    "istanbul-instrumenter-loader": "~0.1.3",
    "karma": "^0.13.3",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.0",
    "karma-coveralls": "^1.1.2",
    "karma-firefox-launcher": "^0.1.4",
    "karma-mocha": "^0.1.10",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.1",
    "react": "~0.14.0",
    "react-dom": "~0.14.0",
    "webpack": "~1.12.2"
  },
  "peerDependencies": {
    "react": "~0.14.0",
    "react-dom": "~0.14.0"
  }
}
